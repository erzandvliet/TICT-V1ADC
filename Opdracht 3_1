#include <stdio.h>

int count (int a[], int size, int n){
	int i = 0;
	int x = 0;
	while (i < size) {
		if (a[i] == n) {
			x++;
	}
		i++;
	}
	return x;
}

int main(void)
{
	int n;
	int a[10] = {1,2,3,4,4,4,5};
	
	printf("Kies te tellen getal: ");
	scanf("%d", &n);
	
	printf("Aantal: %d", count(a,10,n));
	
	return 0;
}
